{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/download.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/api/download",
      "methods": ["POST", "OPTIONS"],
      "dest": "/api/download.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "RAPIDAPI_KEY": "1dda0d29d3mshc5f2aacec619c44p16f219jsn99a62a516f98",
    "RAPIDAPI_HOST": "auto-download-all-in-one.p.rapidapi.com"
  }
}